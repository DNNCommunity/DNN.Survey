GO

DROP PROCEDURE IF EXISTS {databaseOwner}{objectQualifier}GetSurveys
GO

DROP PROCEDURE IF EXISTS {databaseOwner}{objectQualifier}UpdateSurvey
GO

DROP PROCEDURE IF EXISTS {databaseOwner}{objectQualifier}SurveyResultData_Delete
