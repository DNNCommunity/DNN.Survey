GO

DROP PROCEDURE IF EXISTS {databaseOwner}{objectQualifier}Surveys_HasVoted
GO

DROP PROCEDURE IF EXISTS {databaseOwner}{objectQualifier}Surveys_CsvExport
GO

DROP PROCEDURE IF EXISTS {databaseOwner}{objectQualifier}SurveyOptions_AddOrChange
GO

DROP PROCEDURE IF EXISTS {databaseOwner}{objectQualifier}AddSurveyOption
GO

DROP PROCEDURE IF EXISTS {databaseOwner}{objectQualifier}DeleteSurveyOption
GO

DROP PROCEDURE IF EXISTS {databaseOwner}{objectQualifier}GetSurveyOptions
GO

